{
  "name": "learn_nodejs",
  "version": "1.0.0",
  "description": "NodeJS learning pro LianHarman",
  "main": "dist/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon",
    "start": "nodemon",
    "build": "rmdir /s /q dist 2>nul & tsc -p tsconfig.build.json && xcopy src\\views dist\\views /E /I /Y",
    "start:prod": "npm run build && node -r module-alias/register dist/app.js",
    "start:debug": "nodemon --inspect=9229 -e ts,tsx --exec node -r ts-node/register -r tsconfig-paths/register ./src/app.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "node_modules",
      "dist"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src/app.ts"
  },
  "keywords": [
    "NodeJ",
    "Express",
    "MonggoDB"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/LienThuan9697/nodejs_express.git"
  },
  "author": "LianHarman",
  "license": "ISC",
  "_moduleAliases": {
    
    "config": "dist/config",
    "controllers": "dist/controllers",
    "models": "dist/models",
    "routes": "dist/routes",
    "services": "dist/services",
    "views": "dist/views",
    "middleware": "dist/middleware",
    "validation": "dist/validation",
    "types": "dist/types"
  },
  "dependencies": {
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@prisma/client": "6.3.0",
    "@quixo3/prisma-session-store": "3.1.13",
    "bcrypt": "5.1.1",
    "cors": "^2.8.5",
    "dotenv": "16.4.7",
    "ejs": "3.1.10",
    "express": "5.0.1",
    "express-session": "1.18.1",
    "jsonwebtoken": "9.0.2",
    "module-alias": "^2.2.3",
    "multer": "1.4.5-lts.1",
    "mysql2": "3.12.0",
    "passport": "0.7.0",
    "passport-local": "1.0.0",
    "uuid": "11.1.0",
    "web3": "^1.10.4",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@types/bcrypt": "5.0.2",
    "@types/cors": "2.8.17",
    "@types/ejs": "3.1.5",
    "@types/express": "5.0.0",
    "@types/express-session": "1.18.1",
    "@types/jsonwebtoken": "9.0.9",
    "@types/multer": "1.4.12",
    "@types/node": "22.10.7",
    "@types/passport": "1.0.17",
    "@types/passport-local": "1.0.38",
    "@types/paypal__checkout-server-sdk": "^1.0.8",
    "hardhat": "^3.0.12",
    "nodemon": "3.1.9",
    "prisma": "6.3.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3"
  }
}
